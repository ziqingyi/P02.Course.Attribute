<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<configSections>
  <section name ="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
</configSections>
<unity>
    <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.COnfiguration.InterceptionConfigurationExtension
                      , Unity.Interception.Configuration"/>
    <containers>
      <container name="aopContainer">
        <extension type="Interception"/>
        <register type="P18.Course.MyAOP.UnityAOPFolder.IUnityUserProcessor,MyAOP" mapTo="P18.Course.MyAOP.UnityAOPFolder.IUnityUserProcessor,MyAOP">
          
          <interceptor type="aopContainer"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.MonitorBehavior,MyAOP"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.LogBeforeBehavior,MyAOP"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.ParameterCheckBehavior,MyAOP"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.CachingBehavior,MyAOP"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.ExceptionLoggingBehavior,MyAOP"/>
          <interceptorBehavior type="P18.Course.MyAOP.UnityAOPFolder.LogAfterBehavior,MyAOP"/>

        </register>
      </container>
    </containers>
</unity>
</configuration>