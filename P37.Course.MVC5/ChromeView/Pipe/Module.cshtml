@using P37.Course.Web.Core.Models;
@{
    Layout = null;
    ViewBag.Title = "Module";
}
    <h2>Module View in Chrome View</h2>
<h3>@ViewBag.Modules</h3>
<table class="table">
    <tr>
        <th>No.</th>

        <th>Name</th>

        <th>TypeName</th>
    </tr>
    @foreach (SysEvent item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.TypeName
            </td>
        </tr>
    }

</table>