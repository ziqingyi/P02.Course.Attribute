<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
  </configSections>
  <superSocket>
    <servers>
      <server name="ChatSocket" textEncoding="UTF-8"
              serverType="P41.Course.SuperSocket.Server.AppServer.ChatServer, P41.Course.SuperSocket.Server.AppServer"
              ip="Any" port="2019"
              maxConnectionNumber="100">
      </server>
      <!-- can config multiple Servers -->
    </servers>
  </superSocket>



  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>